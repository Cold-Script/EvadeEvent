local redzlib = loadstring(game:HttpGet("https://github.com/realredz/RedzLibV5/blob/main/Source.lua?raw=true"))()
local Window = redzlib:MakeWindow({"WinterClaws | Evade ", "version 1.2", "testando-redzLibv5.json"})
Window:AddMinimizeButton({
Button = { Image = "rbxassetid://1049060234", BackgroundTransparency = 0.25 },
Corner = { CornerRadius = UDim.new(0, 5) }
})
local Tabs = {
Main = Window:MakeTab({"Main", "Home"}),
}
Tabs.Main:AddSection({"Farm"})
_G.C1 = -5000
Tabs.Main:AddDropdown({
        Name = "Position X",
	Description = "Position X",
        Options = {"-5000", "-2500", "0", "2500", "5000"},
        MultiSelect = false,
        Default = {"-5000"},
	Callback = function(value)
_G.C1 = value			
end})
_G.C2 = 3500
Tabs.Main:AddDropdown({
        Name = "Position Y",
	Description = "Position Y",
        Options = {"3500", "7000", "10000"},
        MultiSelect = false,
        Default = {"3500"},
	Callback = function(value)
_G.C2 = value			
end})
_G.C3 = -5000
Tabs.Main:AddDropdown({
        Name = "Position Z",
	Description = "Position Z",
        Options = {"-5000", "-2500", "0", "2500", "5000"},
        MultiSelect = false,
        Default = {"-5000"},
	Callback = function(value)
_G.C3 = value			
end})
Tabs.Main:AddToggle({
	Name = "AFK Farm",
	Description = "Teleport Far to AFK",
	Callback=function(value)
_G.AFK = value
while _G.AFK do wait(0.3)
spawn(function()
pcall(function()
game.Players.LocalPlayer.Character:FindFirstChild('HumanoidRootPart').CFrame = CFrame.new(_G.C1, _G.C2, _G.C3)
wait(0.25)
game.Players.LocalPlayer.Character:FindFirstChild('HumanoidRootPart').CFrame = CFrame.new(-50000, _G.C2, -50000)
end)
end)
end
end})
Tabs.Main:AddToggle({
	Name = "Tickets Farm",
	Description = "Teleport to Tickets",
	Callback=function(value)
_G.AutoTickets = value
while _G.AutoTickets do wait(1)
spawn(function()
pcall(function()
if workspace.Game.Effects.Tickets.Ticket1.HumanoidRootPart then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Game.Effects.Tickets.Ticket1.HumanoidRootPart.CFrame
workspace.Game.Effects.Tickets.Ticket1.HumanoidRootPart.Size = Vector3.new(50, 50, 50)
end
if workspace.Game.Effects.Tickets.Ticket2.HumanoidRootPart then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Game.Effects.Tickets.Ticket2.HumanoidRootPart.CFrame
workspace.Game.Effects.Tickets.Ticket2.HumanoidRootPart.Size = Vector3.new(50, 50, 50)
end
if workspace.Game.Effects.Tickets.Ticket3.HumanoidRootPart then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Game.Effects.Tickets.Ticket3.HumanoidRootPart.CFrame
workspace.Game.Effects.Tickets.Ticket3.HumanoidRootPart.Size = Vector3.new(50, 50, 50)
end
if workspace.Game.Effects.Tickets.Ticket4.HumanoidRootPart then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Game.Effects.Tickets.Ticket4.HumanoidRootPart.CFrame
workspace.Game.Effects.Tickets.Ticket4.HumanoidRootPart.Size = Vector3.new(50, 50, 50)
end
if workspace.Game.Effects.Tickets.Ticket5.HumanoidRootPart then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Game.Effects.Tickets.Ticket5.HumanoidRootPart.CFrame
workspace.Game.Effects.Tickets.Ticket5.HumanoidRootPart.Size = Vector3.new(50, 50, 50)
end
if workspace.Game.Effects.Tickets.Ticket6.HumanoidRootPart then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Game.Effects.Tickets.Ticket6.HumanoidRootPart.CFrame
workspace.Game.Effects.Tickets.Ticket6.HumanoidRootPart.Size = Vector3.new(50, 50, 50)
end
if workspace.Game.Effects.Tickets.Ticket7.HumanoidRootPart then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Game.Effects.Tickets.Ticket7.HumanoidRootPart.CFrame
workspace.Game.Effects.Tickets.Ticket7.HumanoidRootPart.Size = Vector3.new(50, 50, 50)
end
if workspace.Game.Effects.Tickets.Ticket8.HumanoidRootPart then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Game.Effects.Tickets.Ticket8.HumanoidRootPart.CFrame
workspace.Game.Effects.Tickets.Ticket8.HumanoidRootPart.Size = Vector3.new(50, 50, 50)
end
if workspace.Game.Effects.Tickets.Ticket9.HumanoidRootPart then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Game.Effects.Tickets.Ticket9.HumanoidRootPart.CFrame
workspace.Game.Effects.Tickets.Ticket9.HumanoidRootPart.Size = Vector3.new(50, 50, 50)
end
if workspace.Game.Effects.Tickets.Ticket10.HumanoidRootPart then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Game.Effects.Tickets.Ticket10.HumanoidRootPart.CFrame
workspace.Game.Effects.Tickets.Ticket10.HumanoidRootPart.Size = Vector3.new(50, 50, 50)
end
if workspace.Game.Effects.Tickets.Ticket11.HumanoidRootPart then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Game.Effects.Tickets.Ticket11.HumanoidRootPart.CFrame
workspace.Game.Effects.Tickets.Ticket11.HumanoidRootPart.Size = Vector3.new(50, 50, 50)
end
if workspace.Game.Effects.Tickets.Ticket12.HumanoidRootPart then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Game.Effects.Tickets.Ticket12.HumanoidRootPart.CFrame
workspace.Game.Effects.Tickets.Ticket12.HumanoidRootPart.Size = Vector3.new(50, 50, 50)
end
if workspace.Game.Effects.Tickets.Ticket13.HumanoidRootPart then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Game.Effects.Tickets.Ticket13.HumanoidRootPart.CFrame
workspace.Game.Effects.Tickets.Ticket12.HumanoidRootPart.Size = Vector3.new(50, 50, 50)
