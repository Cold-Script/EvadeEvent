local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()
local Window = Library:NewWindow("Evade | v1.2")
local HolidayEvent = Window:NewSection("Holiday Event")
HolidayEvent:Button("Claim Ticket", function()
task.spawn(function()
while wait() do
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Game.Effects.Tickets.Ticket1.HumanoidRootPart.CFrame
end
end)
task.spawn(function()
while wait() do
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Game.Effects.Tickets.Ticket2.HumanoidRootPart.CFrame
end
end)
