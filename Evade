HolidayEvent:Toggle("Attribute Farm", function(v)
_G.AttributeFarm = v
task.spawn(function()
while _G.AttributeFarm and wait(0.35) do
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(500, 2000, 500)
wait(0.2)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-500, 2000, -500)
end
end)
end)
